<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="com.arthur.web.pojo.protal.TUdpUser" table="t_udp_user" schema="myweb">
        <id name="id" column="id"/>
        <property name="userCode" column="user_code"/>
        <property name="password" column="password"/>
        <property name="privilege" column="privilege"/>
        <property name="createdTime" column="created_time"/>
        <property name="opdateTime" column="opdate_time"/>
        <property name="gender" column="gender"/>
        <property name="certificateNumber" column="certificate_number"/>
        <property name="mobile" column="mobile"/>
        <property name="telephoneNumber" column="telephone_number"/>
        <property name="mail" column="mail"/>
        <property name="biography" column="biography"/>
        <property name="name" column="name"/>
        <property name="nickName" column="nickName"/>
        <property name="birthday" column="birthday"/>
    </class>
</hibernate-mapping>